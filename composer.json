{
	"name": "jumilla/source-generator",
	"type": "library",
	"description": "PHP source code generator. Can be written using a method chain.",
	"license": "MIT",
	"keywords": ["php", "source", "code", "generator"],
	"homepage": "http://jumilla.me",
	"authors": [
		{
			"name": "Fumio Furukawa",
			"email": "fumio@jumilla.me"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"league/flysystem": "^1.0"
	},
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0",
        "mockery/mockery": "~0.9"
    },
	"autoload": {
		"psr-4": {
			"Jumilla\\Generators\\": "sources/"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
